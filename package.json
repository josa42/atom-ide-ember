{
  "name": "languageserver-ember",
  "main": "./lib/main",
  "version": "0.3.1",
  "description": "Provide Ember.js support via the Ember Language Server",
  "repository": "https://github.com/josa42/atom-languageserver-ember",
  "license": "MIT",
  "author": "Josa Gesell <josa@gesell.me>",
  "contributors": [
    "Josa Gesell <josa@gesell.me>"
  ],
  "keywords": [
    "ember",
    "ember.js",
    "htmlbars",
    "languageserver"
  ],
  "bugs": {
    "url" : "https://github.com/josa42/atom-languageserver-ember/issues"
  },
  "engines": {
    "atom": ">=1.17.0"
  },
  "activationHooks": [
     "language-javascript:grammar-used",
     "language-mustache:grammar-used",
     "language-ember-htmlbars:grammar-used"
   ],
  "dependencies": {
    "@emberwatch/ember-language-server": "git://github.com/josa42/ember-language-server#7dad67655023b0935f7133adc485ccef57565938",
    "atom-languageclient": "~0.1.1"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    }
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "scripts": {
    "test": "eslint ."
  }
}
