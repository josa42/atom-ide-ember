{
  "name": "languageserver-ember",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "Provide Ember.js support via the Ember Language Server",
  "repository": "https://github.com/josa42/atom-languageserver-ember",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0"
  },
  "dependencies": {
    "@emberwatch/ember-language-server": "git://github.com/josa42/ember-language-server#d1486e92d1682cedd5cde3628ac762e65a30088e",
    "atom-languageclient": "git://github.com/atom/atom-languageclient.git#6d30ebd4e8e400cee2839925cb86eaa14dbe5416"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    }
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "scripts": {
    "test": "eslint ."
  }
}
